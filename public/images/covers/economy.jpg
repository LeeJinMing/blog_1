<?xml version="1.0" encoding="UTF-8"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景 -->
  <rect width="800" height="400" fill="#15803d" />

  <!-- 图表格纹背景 -->
  <g opacity="0.1" stroke="white" stroke-width="1">
    <!-- 横向网格线 -->
    <line x1="0" y1="50" x2="800" y2="50" />
    <line x1="0" y1="100" x2="800" y2="100" />
    <line x1="0" y1="150" x2="800" y2="150" />
    <line x1="0" y1="200" x2="800" y2="200" />
    <line x1="0" y1="250" x2="800" y2="250" />
    <line x1="0" y1="300" x2="800" y2="300" />
    <line x1="0" y1="350" x2="800" y2="350" />
  </g>

  <!-- 金融图表元素 -->
  <g opacity="0.7">
    <!-- 柱状图 -->
    <rect x="150" y="250" width="40" height="100" fill="white" opacity="0.6" />
    <rect x="250" y="200" width="40" height="150" fill="white" opacity="0.7" />
    <rect x="350" y="150" width="40" height="200" fill="white" opacity="0.8" />
    <rect x="450" y="180" width="40" height="170" fill="white" opacity="0.9" />
    <rect x="550" y="100" width="40" height="250" fill="white" opacity="1" />

    <!-- 折线图 -->
    <polyline points="150,280 250,220 350,180 450,240 550,120" fill="none" stroke="white" stroke-width="4"
      stroke-linecap="round" stroke-linejoin="round" />
    <circle cx="150" cy="280" r="6" fill="#f0f9ff" />
    <circle cx="250" cy="220" r="6" fill="#f0f9ff" />
    <circle cx="350" cy="180" r="6" fill="#f0f9ff" />
    <circle cx="450" cy="240" r="6" fill="#f0f9ff" />
    <circle cx="550" cy="120" r="6" fill="#f0f9ff" />
  </g>

  <!-- 金融符号 -->
  <g opacity="0.4">
    <text x="160" y="140" font-family="Arial, sans-serif" font-size="40" fill="white">$</text>
    <text x="620" y="140" font-family="Arial, sans-serif" font-size="40" fill="white">€</text>
    <text x="220" y="340" font-family="Arial, sans-serif" font-size="40" fill="white">¥</text>
    <text x="560" y="340" font-family="Arial, sans-serif" font-size="40" fill="white">£</text>
  </g>

  <!-- 标题文字 -->
  <text x="400" y="80" font-family="Arial, sans-serif" font-size="56" font-weight="bold" text-anchor="middle"
    fill="white">商业经济</text>
  <text x="400" y="120" font-family="Arial, sans-serif" font-size="24" text-anchor="middle" fill="white">洞察全球经济趋势</text>
</svg>