<?xml version="1.0" encoding="UTF-8"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景 -->
  <rect width="800" height="400" fill="#2563eb" />

  <!-- 科技网格图案 -->
  <g opacity="0.15" stroke="white" stroke-width="1">
    <!-- 水平线 -->
    <line x1="0" y1="50" x2="800" y2="50" />
    <line x1="0" y1="100" x2="800" y2="100" />
    <line x1="0" y1="150" x2="800" y2="150" />
    <line x1="0" y1="200" x2="800" y2="200" />
    <line x1="0" y1="250" x2="800" y2="250" />
    <line x1="0" y1="300" x2="800" y2="300" />
    <line x1="0" y1="350" x2="800" y2="350" />

    <!-- 垂直线 -->
    <line x1="100" y1="0" x2="100" y2="400" />
    <line x1="200" y1="0" x2="200" y2="400" />
    <line x1="300" y1="0" x2="300" y2="400" />
    <line x1="400" y1="0" x2="400" y2="400" />
    <line x1="500" y1="0" x2="500" y2="400" />
    <line x1="600" y1="0" x2="600" y2="400" />
    <line x1="700" y1="0" x2="700" y2="400" />
  </g>

  <!-- 电路图案 -->
  <g opacity="0.6" fill="none" stroke="white" stroke-width="3">
    <path d="M150,200 C250,100 400,300 650,150" />
    <path d="M150,250 C300,320 450,180 650,250" />
    <circle cx="150" cy="200" r="8" fill="white" />
    <circle cx="650" cy="150" r="8" fill="white" />
    <circle cx="150" cy="250" r="8" fill="white" />
    <circle cx="650" cy="250" r="8" fill="white" />
    <circle cx="400" cy="225" r="30" fill="none" stroke="white" stroke-width="2" />
  </g>

  <!-- 标题文字 -->
  <text x="400" y="200" font-family="Arial, sans-serif" font-size="56" font-weight="bold" text-anchor="middle"
    fill="white">Technology</text>
  <text x="400" y="240" font-family="Arial, sans-serif" font-size="24" text-anchor="middle"
    fill="white">Exploring Future Innovation</text>
</svg>