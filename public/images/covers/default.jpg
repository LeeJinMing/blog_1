<?xml version="1.0" encoding="UTF-8"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- 渐变背景 -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#4a6fa5" />
      <stop offset="100%" stop-color="#2a4365" />
    </linearGradient>
  </defs>

  <!-- 背景 -->
  <rect width="800" height="400" fill="url(#bg-gradient)" />

  <!-- 装饰图形 -->
  <g opacity="0.1" fill="white">
    <circle cx="100" cy="100" r="50" />
    <circle cx="700" cy="100" r="70" />
    <circle cx="200" cy="300" r="60" />
    <circle cx="600" cy="320" r="40" />
  </g>

  <!-- 中心图形 -->
  <g transform="translate(400, 200)">
    <!-- 外环 -->
    <circle cx="0" cy="0" r="100" fill="none" stroke="white" stroke-width="2" opacity="0.7" />

    <!-- 内部图形 -->
    <g opacity="0.9">
      <polygon points="0,-60 -52,-18 -32,48 32,48 52,-18" fill="white" />
      <circle cx="0" cy="0" r="20" fill="#4a6fa5" stroke="white" stroke-width="2" />
    </g>
  </g>

  <!-- 装饰线条 -->
  <line x1="150" y1="50" x2="650" y2="50" stroke="white" stroke-width="2" opacity="0.5" />
  <line x1="200" y1="350" x2="600" y2="350" stroke="white" stroke-width="2" opacity="0.5" />

  <!-- 标题文字 -->
  <text x="400" y="180" font-family="Arial, sans-serif" font-size="56" font-weight="bold" text-anchor="middle"
    fill="white">Insights 博客</text>
  <text x="400" y="230" font-family="Arial, sans-serif" font-size="24" text-anchor="middle" fill="white">深度洞察与分析</text>
</svg>